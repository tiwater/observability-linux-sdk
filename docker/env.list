# Base URL as used in swupdate and ticosd for reporting to Ticos
TICOS_BASE_URL=https://device.ticos.com

# Ticos's Project key, taken from https://ticos.io/project-key
TICOS_PROJECT_KEY=INSERT_PROJECT_KEY_HERE

# Device serial to report to Ticos
TICOS_DEVICE_ID=qemu-tester

# Software version identifier to use for the binary we'll build and load on the QEMU device
TICOS_SOFTWARE_VERSION=0.0.1

# Hardware version to report to Ticos
TICOS_HARDWARE_VERSION=qemuarm64

# Software type to report to Ticos
TICOS_SOFTWARE_TYPE=main

# Machine to build the example image for
MACHINE=qemuarm64
